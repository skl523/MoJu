" class=\"clearfix\">\r\n            <span>个人信息</span>\r\n          </div>\r\n          <div>\r\n            <div class=\"text-center\">\r\n              <userAvatar :user=\"user\" />\r\n            </div>\r\n            <ul class=\"list-group list-group-striped\">\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"user\" />登陆账号\r\n                <div class=\"pull-right\">{{ user.userName }}</div>\r\n              </li>\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"phone\" />手机号码\r\n                <div class=\"pull-right\">{{ user.phonenumber }}</div>\r\n              </li>\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"phone\" />证件号\r\n                <div class=\"pull-right\">{{ user.idCard }}</div>\r\n              </li>\r\n\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"peoples\" />家庭住址\r\n                <div class=\"pull-right\">{{ user.homeSite }}</div>\r\n              </li>\r\n              \r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"tree\" />年龄\r\n                <div class=\"pull-right\">{{ user.age }}</div>\r\n              </li>\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"email\" />用户邮箱\r\n                <div class=\"pull-right\">{{ user.email }}</div>\r\n              </li>\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"peoples\" />所属角色\r\n                <div class=\"pull-right\">{{ roleGroup }}</div>\r\n              </li>\r\n              <li class=\"list-group-item\">\r\n                <svg-icon icon-class=\"date\" />注册日期\r\n                <div class=\"pull-right\">{{ user.createTime }}</div>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"18\" :xs=\"24\">\r\n    