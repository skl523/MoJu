>\r\n      <el-form-item label=\"名称\" prop=\"name\">\r\n        <el-input\r\n          v-model=\"queryParams.name\"\r\n          placeholder=\"请输入名称\"\r\n          clearable\r\n          size=\"small\"\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"分类名称\" prop=\"typeName\">\r\n        <el-select v-model=\"queryParams.typeName\" placeholder=\"请选择分类名称\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in typeNameOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"适用对象\" prop=\"flock\">\r\n        <el-select v-model=\"queryParams.flock\" placeholder=\"请选择适用对象\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in flockOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"适用场景\" prop=\"scenes\">\r\n        <el-select v-model=\"queryParams.scenes\" placeholder=\"请选择适用场景\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in scenesOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"风格\" prop=\"style\">\r\n        <el-select v-model=\"queryParams.style\" placeholder=\"请选择风格\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in styleOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"其他标签\" prop=\"other\">\r\n        <el-select v-model=\"queryParams.other\" placeholder=\"请选择其他标签\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in otherOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"是否推荐\" prop=\"reco\">\r\n        <el-select v-model=\"queryParams.reco\" placeholder=\"请选择是否推荐\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in recoOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"销售计划\" prop=\"plan\">\r\n        <el-select v-model=\"queryParams.plan\" placeholder=\"请选择销售计划\" clearable size=\"small\">\r\n          <el-option\r\n            v-for=\"dict in planOptions\"\r\n            :key=\"dict.dictValue\"\r\n            :label=\"dict.dictLabel\"\r\n            :value=\"dict.dictValue\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\r\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"primary\"\r\n          plain\r\n          icon=\"el-icon-plus\"\r\n          size=\"mini\"\r\n          @click=\"handleAdd\"\r\n          v-hasPermi=\"['system:goods:add']\"\r\n        >新增</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"success\"\r\n          plain\r\n          icon=\"el-icon-edit\"\r\n          size=\"mini\"\r\n          :disabled=\"single\"\r\n          @click=\"handleUpdate\"\r\n          v-hasPermi=\"['system:goods:edit']\"\r\n        >修改</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"danger\"\r\n          plain\r\n          icon=\"el-icon-delete\"\r\n          size=\"mini\"\r\n          :disabled=\"multiple\"\r\n          @click=\"handleDelete\"\r\n          v-hasPermi=\"['system:goods:remove']\"\r\n        >删除</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"warning\"\r\n          plain\r\n          icon=\"el-icon-download\"\r\n          size=\"mini\"\r\n          :loading=\"exportLoading\"\r\n          @click=\"handleExport\"\r\n          v-hasPermi=\"['system:goods:export']\"\r\n        >导出</el-button>\r\n      </el-col>\r\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"goodsList\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n      <el-table-column label=\"id\" align=\"center\" prop=\"id\" />\r\n      <el-table-column label=\"名称\" align=\"center\" prop=\"name\" width=\"200\" >\r\n           <template slot-scope=\"scope\" v-if=\"scope.row.name\">\r\n            <el-popover trigger=\"hover\" placement=\"top\">\r\n              <div style=\"width:450px;\">{{ scope.row.name }}</div>\r\n              <div slot=\"reference\" class=\"name-wrapper\">\r\n                <el-tag size=\"medium\">{{ scope.row.name }}</el-tag>\r\n              </div>\r\n            </el-popover>\r\n          </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"价格\" align=\"center\" prop=\"price\" >\r\n           <template slot-scope=\"scope\" v-if=\"scope.row.price\">\r\n             ￥{{scope.row.price}}\r\n          </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"图片\" align=\"center\" prop=\"url\">\r\n        <template slot-scope=\"scope\">\r\n            <el-image style=\"width: 100px; height: 100px\" :src=\"baseUrl+scope.row.url\" >\r\n          </el-image>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"数量\" align=\"center\" prop=\"num\" />\r\n      <el-table-column label=\"描述\" align=\"center\" prop=\"remark\" >\r\n           <template slot-scope=\"scope\" v-if=\"scope.row.remark\">\r\n            <el-popover trigger=\"hover\" placement=\"top\">\r\n              <div style=\"width:450px;\">{{ scope.row.remark }}</div>\r\n              <div slot=\"reference\" class=\"name-wrapper\">\r\n                <el-tag size=\"medium\">{{ scope.row.remark }}</el-tag>\r\n              </div>\r\n            </el-popover>\r\n          </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"分类标签\" align=\"center\" prop=\"typeName\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"typeNameOptions\" :value=\"scope.row.typeName\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"年龄范围\" align=\"center\" prop=\"ageStart\" >\r\n        <template slot-scope=\"scope\">\r\n          {{scope.row.ageStart}} ~ {{scope.row.ageEnd}}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"适用对象\" align=\"center\" prop=\"flock\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"flockOptions\" :value=\"scope.row.flock\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"适用场景\" align=\"center\" prop=\"scenes\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"scenesOptions\" :value=\"scope.row.scenes\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"风格\" align=\"center\" prop=\"style\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"styleOptions\" :value=\"scope.row.style\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"其他标签\" align=\"center\" prop=\"other\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"otherOptions\" :value=\"scope.row.other\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"推荐\" align=\"center\" prop=\"reco\">\r\n          <template slot-scope=\"scope\">\r\n            <el-switch\r\n              v-model=\"scope.row.reco\"\r\n              active-value=\"是\"\r\n              inactive-value=\"否\"\r\n              @change=\"handleStatusChange(scope.row)\"\r\n            ></el-switch>\r\n          </template>\r\n        <!-- <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"recoOptions\" :value=\"scope.row.reco\"/>\r\n        </template> -->\r\n      </el-table-column>\r\n      <el-table-column label=\"销售计划\" align=\"center\" prop=\"plan\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"planOptions\" :value=\"scope.row.plan\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"限制时间\" align=\"center\" prop=\"endTime\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ parseTime(scope.row.endTime, '{y}-{m}-{d} {h}:{m}:{s}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"秒杀价格\" align=\"center\" prop=\"prefPrice\" />\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\" fixed=\"right\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-success\"\r\n            @click=\"handleUpdatePlan(scope.row)\"\r\n            v-hasPermi=\"['system:goods:edit']\"\r\n          >设置销售计划</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-edit\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-hasPermi=\"['system:goods:edit']\"\r\n          >修改</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"handleDelete(scope.row)\"\r\n            v-hasPermi=\"['system:goods:remove']\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    \r\n    <pagination\r\n      v-show=\"total>0\"\r\n      :total=\"total\"\r\n      :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    />\r\n\r\n    <!-- 添加或修改礼品（包含秒杀，限时）对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"名称\" prop=\"name\">\r\n          <el-input v-model=\"form.name\" placeholder=\"请输入名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"礼品价格\" prop=\"price\">\r\n          <el-input v-model=\"form.price\" placeholder=\"请输入礼品价格\" type=\"number\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"图片\">\r\n          <imageUpload v-model=\"form.url\" :limit=\"1\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"剩余数量\" prop=\"num\">\r\n          <el-input v-model=\"form.num\" placeholder=\"请输入剩余数量\" type=\"num\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"描述\" prop=\"remark\">\r\n          <el-input v-model=\"form.remark\" type=\"textarea\" placeholder=\"请输入内容\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"分类名称\" prop=\"typeName\">\r\n          <el-select v-model=\"form.typeName\" placeholder=\"请选择分类名称\">\r\n            <el-option\r\n              v-for=\"dict in typeNameOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄范围\" prop=\"ageStart\">\r\n          <el-select v-model=\"form.ageStart\" placeholder=\"请选择年龄范围\">\r\n            <el-option :label=\"item\" :value=\"item\"  v-for=\"(item, index) in 100\" :disabled=\"form.ageEnd && item >= form.ageEnd\" :key=\"index\"/>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄范围\" prop=\"ageEnd\">\r\n          <el-select v-model=\"form.ageEnd\" placeholder=\"请选择年龄范围\">\r\n              <el-option :label=\"item\" :value=\"item\"  v-for=\"(item, index) in 100\" :key=\"index\" :disabled=\"item <= form.ageStart\"/>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"适用对象\" prop=\"flock\">\r\n          <el-select v-model=\"form.flock\" placeholder=\"请选择适用对象\">\r\n            <el-option\r\n              v-for=\"dict in flockOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"适用场景\" prop=\"scenes\">\r\n          <el-select v-model=\"form.scenes\" placeholder=\"请选择适用场景\">\r\n            <el-option\r\n              v-for=\"dict in scenesOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"风格\" prop=\"style\">\r\n          <el-select v-model=\"form.style\" placeholder=\"请选择风格\">\r\n            <el-option\r\n              v-for=\"dict in styleOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"其他标签\" prop=\"other\">\r\n          <el-select v-model=\"form.other\" placeholder=\"请选择其他标签\">\r\n            <el-option\r\n              v-for=\"dict in otherOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"是否推荐\" prop=\"reco\">\r\n          <el-select v-model=\"form.reco\" placeholder=\"请选择是否推荐\">\r\n            <el-option\r\n              v-for=\"dict in recoOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <!-- 添加或修改礼品（包含秒杀，限时）对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"openPlan\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"名称\" prop=\"name\">\r\n          <el-input v-model=\"form.name\" placeholder=\"请输入名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"礼品价格\" prop=\"price\">\r\n          <el-input v-model=\"form.price\" placeholder=\"请输入礼品价格\" type=\"number\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"图片\">\r\n          <imageUpload v-model=\"form.url\" :limit=\"1\"/>\r\n        </el-form-item>\r\n         <el-form-item label=\"销售计划\" prop=\"plan\">\r\n          <el-select v-model=\"form.plan\" placeholder=\"请选择销售计划\">\r\n            <el-option\r\n              v-for=\"dict in planOptions\"\r\n              :key=\"dict.dictValue\"\r\n              :label=\"dict.dictLabel\"\r\n              :value=\"dict.dictValue\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"限制时间\" prop=\"endTime\" v-if=\"form.plan ==='限时抢购'\">\r\n          <el-date-picker clearable size=\"small\"\r\n            v-model=\"form.endTime\"\r\n            type=\"datetime\"\r\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n            placeholder=\"选择限制时间\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"秒杀价格\" prop=\"prefPrice\" v-if=\"form.plan ==='秒杀'\">\r\n          <el-input v-model=\"form.prefPrice\" placeholder=\"请输入秒杀价格\" type=\"number\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitPlanFrom\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { listGoods, getGoods, delGoods, addGoods, updateGoods, exportGoods, changeGoodsReco, updatePlanGoods} from \"@/api/system/goods\";\r\n\r\nexport default {\r\n  name: \"Goods\",\r\n  data() {\r\n    return {\r\n      baseUrl: this.$serviceUrl,\r\n      // 遮罩层\r\n      loading: true,\r\n      // 导出遮罩层\r\n      exportLoading: false,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 礼品（包含秒杀，限时）表格数据\r\n      goodsList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      openPlan: false, \r\n      // 分类名称字典\r\n      typeNameOptions: [],\r\n      // 适用对象(字典)字典\r\n      flockOptions: [],\r\n      // 适用场景字典\r\n      scenesOptions: [],\r\n      // 风格字典\r\n      styleOptions: [],\r\n      // 其他标签字典\r\n      otherOptions: [],\r\n      // 是否推荐字典\r\n      recoOptions: [],\r\n      // 销售计划字典\r\n      planOptions: [],\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: null,\r\n        price: null,\r\n        url: null,\r\n        num: null,\r\n        typeId: null,\r\n        typeName: null,\r\n        ageStart: null,\r\n        ageEnd: null,\r\n        sex: null,\r\n        flock: null,\r\n        scenes: null,\r\n        style: null,\r\n        other: null,\r\n        reco: null,\r\n        plan: null,\r\n        endTime: null,\r\n        prefPrice: null\r\n      },\r\n      // 用户状态修改\r\n      handleStatusChange(row) {\r\n        let text = row.reco =='是'?'推荐':'取消推荐';\r\n        this.$confirm('确认要 : '+text, \"警告\", {\r\n            confirmButtonText: \"确定\",\r\n            cancelButtonText: \"取消\",\r\n            type: \"warning\"\r\n          }).then(function() {\r\n            console.log(\"状态变更：\"+row.reco);\r\n            return changeGoodsReco(row.id, row.reco);\r\n          }).then(() => {\r\n            this.msgSuccess(text + \"成功\");\r\n          }).catch(function() {\r\n            row.reco = row.reco===\"是\"?\"否\":\"是\"\r\n          });\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n    this.getDicts(\"t_goods_type\").then(response => {\r\n      this.typeNameOptions = response.data;\r\n    });\r\n    this.getDicts(\"t_goods_flock\").then(response => {\r\n      this.flockOptions = response.data;\r\n    });\r\n    this.getDicts(\"t_goods_scenes\").then(response => {\r\n      this.scenesOptions = response.data;\r\n    });\r\n    this.getDicts(\"t_goods_style\").then(response => {\r\n      this.styleOptions = response.data;\r\n    });\r\n    this.getDicts(\"t_goods_other\").then(response => {\r\n      this.otherOptions = response.data;\r\n    });\r\n    this.getDicts(\"t_goods_reco\").then(response => {\r\n      this.recoOptions = response.data;\r\n    });\r\n    this.getDicts(\"t_goods_plan\").then(response => {\r\n      this.planOptions = response.data;\r\n    });\r\n  },\r\n  methods: {\r\n    /** 查询礼品（包含秒杀，限时）列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listGoods(this.queryParams).then(response => {\r\n        this.goodsList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.openPlan = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        name: null,\r\n        price: null,\r\n        url: null,\r\n        num: null,\r\n        remark: null,\r\n        typeId: null,\r\n        typeName: null,\r\n        ageStart: null,\r\n        ageEnd: null,\r\n        sex: null,\r\n        flock: null,\r\n        scenes: null,\r\n        style: null,\r\n        other: null,\r\n        createTime: null,\r\n        updateTime: null,\r\n        reco: null,\r\n        plan: null,\r\n        endTime: null,\r\n        prefPrice: null\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 搜索按钮操作 */\r\n    handleQuery() {\r\n      this.queryParams.pageNum = 1;\r\n      this.getList();\r\n    },\r\n    /** 重置按钮操作 */\r\n    resetQuery() {\r\n      this.resetForm(\"queryForm\");\r\n      this.handleQuery();\r\n    },\r\n    // 多选框选中数据\r\n    handleSelectionChange(selection) {\r\n      this.ids = selection.map(item => item.id)\r\n      this.single = selection.length!==1\r\n      this.multiple = !selection.length\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加礼品\";\r\n    },\r\n    handleUpdatePlan(row){\r\n      this.reset();\r\n      const id = row.id || this.ids\r\n      getGoods(id).then(response => {\r\n        this.form = response.data;\r\n        this.openPlan = true;\r\n        this.title = \"设置销售计划\";\r\n      });\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      const id = row.id || this.ids\r\n      getGoods(id).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改礼品\";\r\n      });\r\n    },\r\n    submitPlanFrom(){\r\n        updatePlanGoods(this.form).then(response => {\r\n          this.msgSuccess(\"设置计划成功\");\r\n          this.open = false;\r\n          this.openPlan = false;\r\n          this.getList();\r\n        });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.id != null) {\r\n            updateGoods(this.form).then(response => {\r\n              this.msgSuccess(\"修改成功\");\r\n              this.open = false;\r\n              this.openPlan = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addGoods(this.form).then(response => {\r\n              this.msgSuccess(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      const ids = row.id || this.ids;\r\n      this.$confirm('是否确认删除礼品（包含秒杀，限时）编号为\"' + ids + '\"的数据项?', \"警告\", {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }).then(function() {\r\n          return delGoods(ids);\r\n        }).then(() => {\r\n          this.getList();\r\n          this.msgSuccess(\"删除成功\");\r\n        }).catch(() => {});\r\n    },\r\n    /** 导出按钮操作 */\r\n    handleExport() {\r\n      const queryParams = this.queryParams;\r\n      this.$confirm('是否确认导出所有礼品（包含秒杀，限时）数据项?', \"警告\", {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }).then(() => {\r\n          this.exportLoading = true;\r\n          return exportGoods(queryParams);\r\n        }).then(response => {\r\n          this.download(response.msg);\r\n          this.exportLoading = false;\r\n        }).catch(() => {});\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/system/goods"}]}